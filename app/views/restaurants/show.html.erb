<h1>Page de <%= @restaurant.name %></h1>

<%= link_to "Retour list", restaurants_path %>
<%= link_to "Add review", new_restaurant_review_path(@restaurant) %>


<p>Review</p>
<ul>
  <% @restaurant.reviews.each do |review| %>
  <li class="list-group-item"><%= review.rating %> <%= review.content %> </li>
  <% end %>
</ul>
